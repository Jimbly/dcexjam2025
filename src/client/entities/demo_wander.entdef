---
traits:
- id: wander
- id: drawable
  biasL: [-0.25, 0.3]
  biasF: [0, -0.25]
  biasR: [-0.25, 0.5]
- id: drawable_sprite
  anim_data:
    idle:
      frames: [cat-1, cat-2]
      times: 600
    meow:
      frames: [cat-4, cat-2, cat-4, cat-1, cat-4, cat-2]
      times: [100, 200, 100, 200, 100, 200]
      transition_to: idle
      loop: false
  sprite_data:
    atlas: dcex
    filter_min: LINEAR_MIPMAP_LINEAR
    filter_mag: LINEAR
    origin: [0.5, 1]
  scale: 0.4
